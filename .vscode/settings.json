{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true,
        "**/node_modules": true,
        "**/.venv": true,
    },
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.pytestEnabled": true,
    "python.testing.unittestEnabled": false,
    "yaml.schemas": {
        "https://www.artillery.io/schema.json": []
    },
    "autopep8.args": [
        "--max-line-length=120"
    ]
}